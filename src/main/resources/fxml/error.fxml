<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="pane" alignment="TOP_CENTER"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.sora.fx.controllers.ErrorController">
    <children>
        <TextArea fx:id="txtText" editable="false"                         >
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </TextArea>
        <Button fx:id="btn" onAction="#cancel" text="close">
            <VBox.margin>
                <Insets top="40.0" />
            </VBox.margin>
        </Button>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
